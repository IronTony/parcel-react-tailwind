{
  "name": "parcel-react-tailwind",
  "version": "1.0.0",
  "description": "React, Typescript and Tailwind CSS with Parcel",
  "author": "Mattia Panzeri <mattia.panzeri93@gmail.com> (https://github.com/panz3r)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "setup:dev": "./env.sh .env ./dist/env.js",
    "start": "parcel ./public/index.html --open",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "prebuild": "run-s lint test clean",
    "build": "parcel build ./public/index.html",
    "clean": "rimraf ./dist ./.parcel-cache"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@tailwindcss/forms": "^0.3.4",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^10.3.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "parcel-resolver-ignore": "^1.0.2",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "parcelIgnore": [
    "/env.js"
  ]
